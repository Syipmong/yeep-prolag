{  "name": "yeep-language",
  "displayName": "Yeep Programming Language",
  "description": "Syntax highlighting and language support for the Yeep programming language - a simple, interpreted language with variables, arithmetic, and logical operations",
  "version": "1.0.5",
  "publisher": "YipmongSaid",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "yeep",
    "programming",
    "language",
    "syntax",
    "highlighting",
    "interpreter",
    "scripting"
  ],
  "contributes": {
    "languages": [
      {
        "id": "yeep",
        "aliases": [
          "Yeep",
          "yeep"
        ],
        "extensions": [
          ".yeep"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "yeep",
        "scopeName": "source.yeep",
        "path": "./syntaxes/yeep.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Syipmong/yeep-prolag.git"
  },
  "homepage": "https://github.com/Syipmong/yeep-prolag/wiki",
  "bugs": {
    "url": "https://github.com/Syipmong/yeep-prolag/issues"
  },  "license": "MIT"
}
