{
  "name": "yeep-language",
  "displayName": "Yeep Language Support",
  "description": "Syntax highlighting and language support for the Yeep programming language",
  "version": "1.0.0",
  "publisher": "yeep-lang",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "yeep",
    "programming",
    "language",
    "syntax",
    "highlighting"
  ],
  "contributes": {
    "languages": [
      {
        "id": "yeep",
        "aliases": [
          "Yeep",
          "yeep"
        ],
        "extensions": [
          ".yeep"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "yeep",
        "scopeName": "source.yeep",
        "path": "./syntaxes/yeep.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Syipmong/yeep-prolag.git"
  },
  "homepage": "https://github.com/Syipmong/yeep-prolag/wiki",
  "bugs": {
    "url": "https://github.com/Syipmong/yeep-prolag/issues"
  },
  "license": "MIT",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "typescript": "^4.4.0"
  }
}
