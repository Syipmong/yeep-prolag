// Testing current built-in functions
print("=== Testing Built-in Functions ===");

// Math functions
print("Math Functions:");
print("sqrt(16) = " + sqrt(16));
print("abs(-5) = " + abs(-5));
print("pow(2, 3) = " + pow(2, 3));
print("max(1, 5, 3) = " + max(1, 5, 3));
print("min(1, 5, 3) = " + min(1, 5, 3));
print("floor(3.7) = " + floor(3.7));
print("ceil(3.2) = " + ceil(3.2));
print("round(3.6) = " + round(3.6));

// String functions
print("\nString Functions:");
print("length('hello') = " + length("hello"));
print("uppercase('hello') = " + uppercase("hello"));
print("lowercase('WORLD') = " + lowercase("WORLD"));
print("substring('hello', 1, 3) = " + substring("hello", 1, 3));

// Type functions
print("\nType Functions:");
print("type(42) = " + type(42));
print("type('hello') = " + type("hello"));
print("toString(123) = " + toString(123));
print("toNumber('456') = " + toNumber("456"));

// Utility functions
print("\nUtility Functions:");
print("random() = " + random());
print("time() = " + time());
